   "source": [
    "def dfs(node, vis, adj):\n",
    "    vis[node] =  True\n",
    "    ans.append(node)\n",
    "    for it in adj[node]:\n",
    "        if vis[it] == False:\n",
    "            dfs(it, vis, adj)\n",
    "\n",
    "\n",
    "adj = [[1,2,3], [], [4], [], []] \n",
    "V = 5   # No of verices\n",
    "vis = [False]*V\n",
    "ans = []\n",
    "\n",
    "dfs(0, vis, adj)\n",
    "print(ans)"
   ]
